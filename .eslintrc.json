{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "airbnb",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "react/prop-types": "off" // Tắt rule prop-types nếu không dùng
    }
  }